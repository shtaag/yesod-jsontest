
function postTodoJson() {
    alert("postTodoJson called");
    alert($('#todo [name=f2]').val());
    alert($('#todo [name=f3]').val());

    $.ajax({
        url: '/todo',
        type: 'POST',
        datatype: 'json',
        data: {
            text: $('#todo [name=f2]').val(),
            done: $('#todo [name=f3]').val()
        },
        success: function(o) {
            alert(o.status);
        },
        error: function() {
            alert("error in post");
        }
    });


}
